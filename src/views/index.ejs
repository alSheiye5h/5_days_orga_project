    <script>
      // loadiwha
      window.jQuery ||
        document.write(
          '<script src="assets/js/vendor/jquery-2.2.4.min.js"><\/script>'
        );
    </script>
    <script src="/js/contactHandler.js"></script>
    <script src="/js/flash.js"></script>
    <script src="/js/scrollHandler.js"></script>
    <script src="/js/navigationHandler.js"></script>
    <script src="/js/sliderHandler.js"></script>
    <script src="/js/labelTransition.js"></script>
    <script src="/jsmain.js"></script>
    <!-- request dial les ordres -->
    <script src="/script/script.js"></script>
